<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <include domain="file" path="shared/"/>
        <exclude domain="file" path="temp/"/>
    </cloud-backup>
    <device-transfer>
        <include domain="file" path="shared/"/>
        <exclude domain="file" path="temp/"/>
    </device-transfer>
</data-extraction-rules>
